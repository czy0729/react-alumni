{
	"plugins": [
		"transform-decorators-legacy",
		"transform-function-bind",
		"add-module-exports", 
		["transform-runtime", {
			"helpers": false,
			"polyfill": true,
			"regenerator": true
		}],
		["import", {
			"style": "css",
			"libraryName": "antd-mobile"
		}]
	],
	"presets": [
		"react",
		"es2015",
		"stage-2"
	]
}